<!DOCTYPE html>
<html>
  <head>
    <title>GroupProjectOrganizer</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <nav class="top-nav teal accent-4">
        <div class="container">
          <div class="nav-wrapper">
            <a href="#" class="brand-logo center">Group Project Organizer</a>
          </div>
        </div>
        <div class="container">
          <a href="#" data-activates="nav-mobile" class="button-collapse top-nav full hide-on-large-only"><i class="material-icons">menu</i></a>
        </div>
      </nav>
        <ul id="nav-mobile" class="side-nav fixed">
          <%= render partial: 'devise/menu/menu' %>
        </ul>
    </header>
    <div class="container main">
      <div class="row">
        <div class="col s10 offset-s2 grey lighten-4">
          <%= yield %>
        </div>
      </div>
    </div>
    <footer class="branding right" style="padding-right: 10px">
      <small>&copy; 2017 <strong>Group Project Organizer</strong></small>
    </footer>
  </body>
</html>
