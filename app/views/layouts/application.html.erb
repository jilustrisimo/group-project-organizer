<!DOCTYPE html>
<html>
  <head>
    <title>GroupProjectOrganizer</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <nav class="teal accent-4 top-nav">
            <a href="#" class="brand-logo center">Group Project Organizer</a>
      </nav>
      <ul id="nav-mobile" class="side-nav fixed">
        <%= render partial: 'devise/menu/menu' %>
      </ul>
    </header>
    <!--<a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>-->
    <div class="container main">
      <div class="row">
        <div class="col s10 offset-s2 grey lighten-4">
          <%= yield %>
        </div>
      </div>
    </div>
    <!--<div class="fixed-action-btn">-->
    <!--</div>-->
    <footer class="branding">
      <small>&copy; 2017 <strong>Group Project Organizer</strong></small>
    </footer>
  </body>
</html>
